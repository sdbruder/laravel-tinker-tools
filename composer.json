{
    "name": "sdbruder/laravel-tinker-tools",
    "description": "Use short class names in an Artisan tinker session",
    "keywords": [
        "sdbruder",
        "spatie",
        "laravel-tinker-tools",
        "flysystem",
        "dropbox",
        "v2",
        "api"
    ],
    "homepage": "https://github.com/sdbruder/laravel-tinker-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sergio Devojno Bruder",
            "email": "sergio@bruder.com.br",
            "homepage": "http://sergio.bruder.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sdbruder\\TinkerTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sdbruder\\TinkerTools\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
